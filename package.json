{
  "name": "@insurgentlab/commitlint-config",
  "description": "Commitlint config for @insurgentlab/conventional-changelog-preset",
  "version": "18.1.3",
  "author": "Mario Nebl <hello@herebecode.com>",
  "maintainers": [
    "Pierre Cavin <me@sherlox.io>"
  ],
  "license": "MIT",
  "scripts": {
    "lint:eslint": "eslint . --ext .js",
    "lint:prettier": "prettier ./**/*.{json,md,yml} --check",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:fix": "npm run lint:eslint -- --fix && npm run lint:prettier -- --write",
    "test": "jest --coverage",
    "test:commitlint": "commitlint --from HEAD~1 --to HEAD --verbose"
  },
  "engines": {
    "node": ">=v16"
  },
  "dependencies": {
    "@insurgentlab/conventional-changelog-preset": "^7.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "18.0.0",
    "@commitlint/lint": "18.0.0",
    "@insurgentlab/commitlint-config": "file:./",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "11.0.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.2.1",
    "@semantic-release/npm": "11.0.0",
    "@semantic-release/release-notes-generator": "12.0.0",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jest": "27.4.3",
    "eslint-plugin-prettier": "5.0.1",
    "jest": "29.7.0",
    "prettier": "3.0.3",
    "semantic-release": "22.0.5"
  },
  "homepage": "https://github.com/insurgent-lab/commitlint-config#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/insurgent-lab/commitlint-config.git"
  },
  "bugs": {
    "url": "https://github.com/insurgent-lab/commitlint-config/issues"
  },
  "keywords": [
    "conventional-changelog",
    "commitlint",
    "commitlint-config",
    "angular"
  ],
  "files": [
    "index.js"
  ],
  "publishConfig": {
    "access": "public"
  }
}
